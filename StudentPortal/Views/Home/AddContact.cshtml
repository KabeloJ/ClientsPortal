@model Core.User.Models.ContactModel

@{
    ViewData["Title"] = "AddContact";
    Layout = null;
}
    <div class="row">
        <div id="ContactErrors">
        </div>
        <div class="form-group col-md-5 m-1">
            <span id="ContactTypeSpan" class="text-danger small"></span>
            <input id="ContactType" class="form-control" placeholder="Contact Type here" />
        </div>
        <div class="form-group col-md-5 m-1">
            <span id="ContactNumberSpan" class="text-danger small"></span>
            <input id="ContactNumber" class="form-control" placeholder="Contact number here" />
        </div>
        <div class="form-group col-md-2 m-1">
            <input type="button" onclick="SubmitContact()" value="Create" class="btn btn-primary" />
        </div>
    </div>
